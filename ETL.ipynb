    "import requests\n",
    "\n",
    "def get_banks_data():\n",
    "    url = \"https://brasilapi.com.br/api/banks/v1\"\n",
    "    response = requests.get(url)\n",
    "    if response.status_code == 200:\n",
    "        data = response.json()\n",
    "        return data\n",
    "    else:\n",
    "        print(\"Erro ao puxar dados\")\n",
    "        return None"
   ]
